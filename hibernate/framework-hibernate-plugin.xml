<plugin
	name="framework-hibernate"
	package="wbs.framework.hibernate">

	<layer name="hibernate">

		<bootstrap-component class="HibernateComponentPlugin"/>

		<component class="HibernateDatabase"/>
		<component class="HibernateInterceptor"/>
		<component class="HibernateObjectDatabaseHelper"/>
		<component class="HibernateSessionFactory"/>
		<component class="HibernateSessionFactoryBuilder"/>
		<component class="HibernateTransaction"/>

	</layer>

</plugin>
