<record
	name="message-template-entry-value"
	type="minor">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="message-template-set"/>

		<identity-reference-field
			type="message-template-entry-type"/>

		<!-- details -->

		<deleted-field/>

	</fields>

	<collections>

		<children-mapping
			name="fields"
			type="message-template-field-value"
			map-type="integer"
			map-column="message_template_field_type_id"/>

	</collections>

</record>