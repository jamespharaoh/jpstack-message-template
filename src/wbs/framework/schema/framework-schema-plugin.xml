<plugin
	name="framework-schema"
	package="wbs.framework.schema">

	<layer name="schema">
		<component class="helper.SchemaNamesHelperImplementation"/>
		<component class="helper.SchemaTypesHelperImplementation"/>
	</layer>

	<layer name="schema-tool">
		<component class="builder.SchemaFromModel"/>
		<component class="builder.SchemaTableFromModel"/>
		<component class="tool.SchemaToSqlImplementation"/>
		<component class="tool.SchemaToolExceptionLogger"/>
		<component class="tool.SchemaToolSpecialConfig"/>
	</layer>

</plugin>
